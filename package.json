  "license": "ISC",
  "bugs": {
    "url": "https://github.com/futurahaus/shingari/issues"
  },
  "homepage": "https://github.com/futurahaus/shingari#readme",
  "description": "",
  "dependencies": {
    "react-icons": "^5.5.0"
  }
}
